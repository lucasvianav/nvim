        //     ]
        // },
        // {
        //     "key": "=",
        //     "name": "Unfold All",
        //     "type": "command",
        //     "command": "editor.unfoldAll"
        // },
        {
            "key": "t",
            "name": "Toggle Terminal",
            "type": "command",
            "command": "workbench.action.togglePanel",
            "when": "terminalFocus"
        },
        {
            "key": "/",
            "name": "Search in a project",
            "type": "command",
            "command": "workbench.action.findInFiles"
        },
        {
            "key": "g",
            "name": "+Git",
            "type": "bindings",
            "bindings": [
                {
                    "key": "c",
                    "name": "Commit",
                    "type": "command",
                    "command": "git.commit"
                },
                {
                    "key": "f",
                    "name": "Fetch",
                    "type": "command",
                    "command": "git.fetch"
                },
                {
                    "key": "p",
                    "name": "Publish",
                    "type": "command",
                    "command": "git.publish"
                },
                {
                    "key": "s",
                    "name": "Stage",
                    "type": "command",
                    "command": "git.stage"
                },
                {
                    "key": "u",
                    "name": "Unstage",
                    "type": "command",
                    "command": "git.unstage"
                },
                {
                    "key": "h",
                    "name": "Open in GitHub",
                    "type": "command",
                    "command": "gitlens.openFileInRemote"
                },
                {
                    "key": "g",
                    "name": "Display Source Control",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "P",
                    "name": "Pull",
                    "type": "command",
                    "command": "git.pull"
                }
            ]
        },
        {
            "key": "s",
            "name": "Find & Replace in Project",
            "type": "command",
            "command": "workbench.action.replaceInFiles"
        },
        {
            "key": "d",
            "name": "Definition",
            "type": "command",
            "command": "editor.action.revealDefinition",
            "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
        },
        {
            "key": "D",
            "name": "Declaration",
            "type": "command",
            "command": "editor.action.revealDeclaration"
        },
        {
            "key": "E",
            "name": "Errors",
            "type": "command",
            "command": "workbench.actions.view.problems"
        },
        {
            "key": "n",
            "name": "No Highlight",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": ":noh<CR>"
        },
        {
            "key": "?",
            "name": "+Show more...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "e",
                    "name": "Toggle explorer",
                    "type": "command",
                    "command": "workbench.view.explorer",
                    "when": "!explorerViewletVisible"
                },
                {
                    "key": "e",
                    "name": "Toggle explorer",
                    "type": "command",
                    "command": "workbench.action.toggleSidebarVisibility",
                    "when": "explorerViewletVisible"
                },
                {
                    "key": "g",
                    "name": "Show source control",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "t",
                    "name": "Show Terminal",
                    "type": "command",
                    "command": "workbench.action.terminal.toggleTerminal",
                    "when": "!terminalFocus"
                },
                {
                    "key": "x",
                    "name": "Show extensions",
                    "type": "command",
                    "command": "workbench.view.extensions"
                },
                {
                    "key": "o",
                    "name": "Show output",
                    "type": "command",
                    "command": "workbench.action.output.toggleOutput"
                },
            ]
        },
        {
            "key": "o",
            "name": "Enter insert mode in new blank line below",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": "<Leader>o"
        },
        {
            "key": "O",
            "name": "Enter insert mode in new blank line above",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": "<Leader>O"
        },
    ],
    "whichkey.bindingOverrides": [
    
    ],
    "whichkey.sortOrder": "nonNumberFirst",
    "whichkey.delay": 0,
    "editor.hideCursorInOverviewRuler": true,
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "none",
    "editor.emptySelectionClipboard": false,
    "liveshare.showInStatusBar": "whileCollaborating",
