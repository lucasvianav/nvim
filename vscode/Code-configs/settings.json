{
    "C_Cpp.updateChannel": "Insiders",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "cSpell.userWords": [
        "Auxiliar",
        "Avi",
        "Avião",
        "Compara",
        "Comparação",
        "Felps",
        "Moura",
        "Sant'Anna",
        "Troca",
        "Viana",
        "Vilela",
        "bolha",
        "isalpha",
        "isnumeric",
        "o",
        "selecao",
        "swap'em"
    ],
    "git.autofetch": true,
    "workbench.statusBar.visible": true,
    "window.menuBarVisibility": "classic",
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.activityBar.visible": true,
    "editor.minimap.enabled": false,
    "python.languageServer": "Pylance",
    "git.confirmSync": false,
    "editor.linkedEditing": true,
    "liveServer.settings.donotVerifyTags": true,
    "liveServer.settings.donotShowInfoMsg": true,
    "update.mode": "start",
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.renderControlCharacters": false,
    "workbench.iconTheme": "material-icon-theme",
    "explorer.confirmDragAndDrop": false,
    "git.suggestSmartCommit": false,
    "files.autoSave": "off",
    "editor.cursorStyle": "block",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "workbench.startupEditor": "newUntitledFile",
    "editor.lineNumbers": "relative",
    "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim/init.vim",
    "whichkey.bindings": [
        {
            "key": " ",
            "name": "VSCode Commands",
            "type": "command",
            "command": "workbench.action.showCommands"
        },
        {
            "key": "e",
            "name": "Toggle explorer visibility",
            "type": "command",
            "command": "workbench.view.explorer"
        },
        {
            "key": "v",
            "name": "Split editor right",
            "type": "command",
            "command": "workbench.action.splitEditor"
        },
        {
            "key": "h",
            "name": "Split editor below",
            "type": "command",
            "command": "workbench.action.splitEditorDown"
        },
        {
            "key": "i",
            "name": "Organize Imports",
            "type": "command",
            "command": "editor.action.organizeImports"
        },
        {
            "key": "m",
            "name": "+Merge conflict",
            "type": "bindings",
            "bindings": [
                {
                    "key": "b",
                    "name": "Accept both",
                    "type": "command",
                    "command": "merge-conflict.accept.both"
                },
                {
                    "key": "c",
                    "name": "Accept current",
                    "type": "command",
                    "command": "merge-conflict.accept.current"
                },
                {
                    "key": "i",
                    "name": "Accept incoming",
                    "type": "command",
                    "command": "merge-conflict.accept.incoming"
                },
                {
                    "key": "B",
                    "name": "Accept all both",
                    "type": "command",
                    "command": "merge-conflict.accept.all-both"
                },
                {
                    "key": "C",
                    "name": "Accept all current",
                    "type": "command",
                    "command": "merge-conflict.accept.all-current"
                },
                {
                    "key": "I",
                    "name": "Accept all incoming",
                    "type": "command",
                    "command": "merge-conflict.accept.all-incoming"
                },
                {
                    "key": "n",
                    "name": "Next Conflict",
                    "type": "command",
                    "command": "merge-conflict.next"
                },
                {
                    "key": "N",
                    "name": "Previous Conflict",
                    "type": "command",
                    "command": "merge-conflict.previous"
                }
            ]
        },
        {
            "key": "t",
            "name": "Toggle Terminal",
            "type": "command",
            "command": "workbench.action.togglePanel",
            "when": "terminalFocus"
        },
        {
            "key": "/",
            "name": "Search in a project",
            "type": "command",
            "command": "workbench.action.findInFiles"
        },
        {
            "key": "g",
            "name": "+Git",
            "type": "bindings",
            "bindings": [
                {
                    "key": "c",
                    "name": "Commit",
                    "type": "command",
                    "command": "git.commit"
                },
                {
                    "key": "f",
                    "name": "Fetch",
                    "type": "command",
                    "command": "git.fetch"
                },
                {
                    "key": "p",
                    "name": "Publish",
                    "type": "command",
                    "command": "git.publish"
                },
                {
                    "key": "s",
                    "name": "Stage",
                    "type": "command",
                    "command": "git.stage"
                },
                {
                    "key": "u",
                    "name": "Unstage",
                    "type": "command",
                    "command": "git.unstage"
                },
                {
                    "key": "h",
                    "name": "Open in GitHub",
                    "type": "command",
                    "command": "gitlens.openFileInRemote"
                },
                {
                    "key": "g",
                    "name": "Display Source Control",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "P",
                    "name": "Pull",
                    "type": "command",
                    "command": "git.pull"
                }
            ]
        },
        {
            "key": "s",
            "name": "Find & Replace in Project",
            "type": "command",
            "command": "workbench.action.replaceInFiles"
        },
        {
            "key": "d",
            "name": "Definition",
            "type": "command",
            "command": "editor.action.revealDefinition",
            "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
        },
        {
            "key": "D",
            "name": "Declaration",
            "type": "command",
            "command": "editor.action.revealDeclaration"
        },
        {
            "key": "E",
            "name": "Errors",
            "type": "command",
            "command": "workbench.actions.view.problems"
        },
        {
            "key": "n",
            "name": "No Highlight",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": ":noh<CR>"
        },
        {
            "key": "?",
            "name": "+Show more...",
            "type": "bindings",
            "bindings": [
                {
                    "key": "e",
                    "name": "Toggle explorer",
                    "type": "command",
                    "command": "workbench.view.explorer",
                    "when": "!explorerViewletVisible"
                },
                {
                    "key": "e",
                    "name": "Toggle explorer",
                    "type": "command",
                    "command": "workbench.action.toggleSidebarVisibility",
                    "when": "explorerViewletVisible"
                },
                {
                    "key": "g",
                    "name": "Show source control",
                    "type": "command",
                    "command": "workbench.view.scm"
                },
                {
                    "key": "t",
                    "name": "Show Terminal",
                    "type": "command",
                    "command": "workbench.action.terminal.toggleTerminal",
                    "when": "!terminalFocus"
                },
                {
                    "key": "x",
                    "name": "Show extensions",
                    "type": "command",
                    "command": "workbench.view.extensions"
                },
                {
                    "key": "o",
                    "name": "Show output",
                    "type": "command",
                    "command": "workbench.action.output.toggleOutput"
                }
            ]
        },
        {
            "key": "o",
            "name": "Enter insert mode in new blank line below",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": "<Leader>o"
        },
        {
            "key": "O",
            "name": "Enter insert mode in new blank line above",
            "type": "command",
            "command": "vscode-neovim.send",
            "args": "<Leader>O"
        }
    ],
    "whichkey.bindingOverrides": [],
    "whichkey.sortOrder": "nonNumberFirst",
    "whichkey.delay": 0,
    "editor.hideCursorInOverviewRuler": true,
    "editor.renderWhitespace": "none",
    "editor.emptySelectionClipboard": false,
    "liveshare.showInStatusBar": "whileCollaborating",
    "workbench.colorCustomizations": {
        "statusBar.border": "#f7f7f7"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "fold.level": 1,
    "editor.rulers": [
        130
    ],
    "editor.showFoldingControls": "mouseover",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "git-commit",
        "go",
        "graphql",
        "handlebars",
        "haskell",
        "html",
        "jade",
        "java",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "python",
        "pug",
        "restructuredtext",
        "rust",
        "scala",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml",
        "vim"
    ],
    "cSpell.enabled": true,
    "editor.wordWrap": "on",
    "workbench.preferredLightColorTheme": "One Dark Pro",
    "explorer.compactFolders": false,
    "[python]": {
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.wordBasedSuggestions": false
    },
    "python.analysis.completeFunctionParens": true,
    "workbench.colorTheme": "One Dark Pro",
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "[One Dark Pro]": {
            "enabled": true
        }
    }
}